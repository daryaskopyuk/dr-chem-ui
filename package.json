{
  "name": "cra-template",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000",
  "dependencies": {
    "@datarobot/design-system": "3.2.0",
    "@fortawesome/fontawesome-pro": "5.15.1",
    "@reduxjs/toolkit": "1.6.0",
    "@types/react": "17.0.9",
    "@types/react-dom": "17.0.6",
    "@types/react-responsive": "8.0.2",
    "@types/react-router-dom": "5.1.7",
    "axios": "0.21.1",
    "classnames": "2.3.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "node-sass": "6.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "react-responsive": "8.2.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.3",
    "ttag": "1.7.24",
    "typescript": "4.3.2",
    "web-vitals": "2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage",
    "eject": "react-scripts eject",
    "start:coverage": "react-scripts -r @cypress/instrument-cra start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --fix  .",
    "cypress": "CYPRESS_coverage=false npx cypress open",
    "cypress:coverage": "npx cypress open",
    "translations:update": "npx ttag-cli update src/i18n/uk.po src/ --discover=t && npx ttag-cli update src/i18n/ru.po src/ --discover=t",
    "translations:po2json": "npx ttag-cli po2json src/i18n/ru.po > src/i18n/ru.po.json && npx ttag-cli po2json src/i18n/uk.po > src/i18n/uk.po.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/code-coverage": "3.9.6",
    "@cypress/instrument-cra": "1.4.0",
    "@testing-library/jest-dom": "5.12.0",
    "@testing-library/react": "11.2.7",
    "@testing-library/user-event": "13.1.9",
    "@types/jest": "26.0.23",
    "@types/node": "15.9.0",
    "@typescript-eslint/eslint-plugin": "4.26.0",
    "@typescript-eslint/parser": "4.26.0",
    "cypress": "7.4.0",
    "eslint": "7.27.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-cypress": "2.11.3",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-mocha": "9.0.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "4.6.0",
    "istanbul-lib-coverage": "3.0.0",
    "nyc": "15.1.0",
    "prettier": "2.3.0",
    "ttag-cli": "1.9.2"
  }
}
